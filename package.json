    "prepublishOnly": "npm run build",
    "start": "sirv public",
    "test": "start-server-and-test dev http://localhost:5000 cy:run"
  },
  "svelte": "src/components/components.module.js",
  "version": "0.1.2",
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@testing-library/cypress": "5.3.0",
    "autoprefixer": "9.8.6",
    "axios": ">=0.21.1",
    "cypress": "4.4.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-svelte3": "2.7.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "postcss": "7.0.32",
    "postcss-load-config": "2.1.0",
    "prettier": "2.1.1",
    "rollup": "2.26.9",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.0.0",
    "rollup-plugin-terser": "7.0.1",
    "sirv-cli": "^1.0.6",
    "start-server-and-test": "^1.11.0",
    "svelte": "^3.25.0",
    "svelte-preprocess": "4.2.1"
  },
  "files": [
    "src/components",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "svelte"
  ],
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{html, css, scss, stylus, js, ts, json, yml, md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js, svelte}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "vulcangz",
  "bugs": {
    "url": "https://github.com/vulcangz/svelte-baidumaps/issues"
  },
  "homepage": "https://github.com/vulcangz/svelte-baidumaps#readme",
  "dependencies": {}
